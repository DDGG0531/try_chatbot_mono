  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core":"^5.0.7",
    "@types/node": "^24.3.1",
    "esbuild-plugin-tsconfig-paths": "^1.0.1",
    "prettier": "^3.6.2",
